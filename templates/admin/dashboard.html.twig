{% extends '@EasyAdmin/page/content.html.twig' %}
{% block stylesheets %}
	{# <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous"> #}

{% endblock %}

{% block page_title 'Tableau de bord' %}
{% block page_content %}
	<div class="row">
		<div class="col-3 text-center">			
			<div class="card px-5 pt-2">
				<img src="assets/img/users-solid.svg" height="130">
				<div class="card-body h2">
					{{ usersCount }}
				</div>
			</div>
		</div>
		<div class="col-3 text-center counter">
			<div class="card px-5 pt-2">
				<img src="assets/img/calendar-alt-solid.svg" height="130">
				<div class="card-body h2">
					{{ rentalsCount }}
				</div>
			</div>
		</div>
		<div class="col-3 text-center">			
			<div class="card px-5 pt-2">
				<img src="assets/img/car-solid.svg" height="130">
				<div class="card-body h2">
					{{ carsCount }}
				</div>
			</div>
		</div>
	</div>
{% endblock %}
